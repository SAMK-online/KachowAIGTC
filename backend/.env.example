# NVIDIA API Catalog (Primary - get key from build.nvidia.com)
NVIDIA_API_KEY=nvapi-xxxxx
NVIDIA_MODEL=nvidia/llama-3.1-nemotron-70b-instruct

# NVIDIA Riva TTS (Optional - get function ID from build.nvidia.com)
# RIVA_FUNCTION_ID=your_function_id

# ElevenLabs (Fallback TTS)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
ELEVENLABS_VOICE_ID=your_voice_id

# Workspace directory to watch for file changes
WORKSPACE_DIR=/path/to/your/project
